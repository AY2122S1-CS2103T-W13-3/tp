<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: transparent;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="300.0" style="-fx-border-style: solid;" translateX="150.0" translateY="100.0">
        <columns>
          <TableColumn fx:id="statName" editable="false" prefWidth="300.0" sortable="false" style="-fx-font-family: arial;" text="Statistics" />
          <TableColumn fx:id="value" editable="false" minWidth="0.0" prefWidth="300.0" sortable="false" style="-fx-font-family: arial;" text="Value " />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</AnchorPane>
